{
	"name": "Cryptoblocker", 
	"version": "1.0", 
	"description": 
	"Dynamically block cryptocurrency miners", 
	"manifest_version": 2,
	"background": {
		"scripts": ["background.js"],
		"persistent": true
	},
	"permissions": ["storage","<all_urls>","webRequest", "webRequestBlocking"],
	"web_accessible_resources": [
   	"filters.txt"
	]
}